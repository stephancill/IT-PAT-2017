unit User_U;

interface

type
  TUserType = (Teacher=1, Student);
  TUser = Class(TObject)
    private

    public
      email, id: string;
      userType: TUserType;
      constructor Create(email, id, firstname, lastname: string; userType: TUserType); overload;
  end;



implementation

{$R *.dfm}

constructor TUser.Create(email, id, firstname, lastname: string; userType: TUserType);
begin
  self.email := email;
  self.id := id;
  self.userType := userType;
end;

end.
